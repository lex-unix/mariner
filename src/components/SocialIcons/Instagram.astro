---
interface Props {
  width?: number | string
  height?: number | string
  class?: string
}

const { width = 20, height = 20, class: className } = Astro.props
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  width={width}
  height={height}
  viewBox="0 0 20 20"
  fill="none"
  stroke="currentColor"
  stroke-width="2"
  stroke-linecap="round"
  stroke-linejoin="round"
  class={className}
>
  <rect width="16.7" height="16.7" x="1.67" y="1.67" rx="4.17" ry="4.17"></rect>
  <path d="M13.3 9.47A3.33 3.33 0 1 1 10.53 6.67 3.33 3.33 0 0 1 13.3 9.47z"
  ></path>
  <line x1="14.58" x2="14.59" y1="5.42" y2="5.42"></line>
</svg>
